{
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended",
      "plugin:jest-dom/recommended",
      "plugin:react-hooks/recommended",
      "plugin:testing-library/react"
    ],
    "plugins": [
      "react",
      "jest",
      "jest-dom",
      "react-hooks",
      "testing-library"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  }
